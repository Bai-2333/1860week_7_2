CHIP DecoderSegmentF {
    IN A, B, C, D;
    OUT f;

    PARTS:
    // f = (A ∧ C) ∨ (C ∧ ¬D) ∨ (A ∧ B) ∨ (¬B ∧ ¬C ∧ ¬D)
    Not(in=B, out=notB);
    Not(in=C, out=notC);
    Not(in=D, out=notD);

    And(a=A, b=C, out=AC);
    And(a=C, b=notD, out=CnotD);
    And(a=A, b=B, out=AB);
    And(a=notB, b=notC, out=notBnotC);
    And(a=notBnotC, b=notD, out=notBnotCnotD);

    Or(a=AC, b=CnotD, out=term1);
    Or(a=term1, b=AB, out=term2);
    Or(a=term2, b=notBnotCnotD, out=f);
}
